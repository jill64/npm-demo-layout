{
  "name": "@jill64/npm-demo-layout",
  "description": "ðŸŽ¨ My Svelte layout template for npm library demo site",
  "version": "1.0.212",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./highlight/languages": {
      "types": "./dist/highlight/languages.d.ts",
      "svelte": "./dist/highlight/languages.js",
      "default": "./dist/highlight/languages.js"
    },
    "./highlight": {
      "types": "./dist/highlight/index.d.ts",
      "svelte": "./dist/highlight/index.js",
      "default": "./dist/highlight/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "prettier": "@jill64/prettier-config",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run check && npx eslint .",
    "format": "npx @jill64/psx",
    "test": "playwright test"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@jill64/eslint-config-svelte": "1.2.34",
    "@jill64/playwright-config": "2.3.0",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/sentry-sveltekit-cloudflare": "1.7.14",
    "@playwright/test": "1.44.1",
    "@sveltejs/adapter-cloudflare": "4.4.1",
    "@sveltejs/kit": "2.5.10",
    "@sveltejs/vite-plugin-svelte": "3.1.1",
    "@jill64/universal-sanitizer": "1.2.13",
    "svelte": "4.2.18",
    "typescript": "5.4.5",
    "vite": "5.3.1"
  },
  "dependencies": {
    "@jill64/svelte-dark-theme": "2.3.77",
    "@jill64/svelte-menu": "1.0.22",
    "@jill64/svelte-ogp": "1.1.28",
    "@jill64/svelte-toast": "1.3.41",
    "@jill64/svelte-sanitize": "1.2.20",
    "svelte-code-copy": "1.0.27",
    "svelte-highlight": "7.6.1",
    "svelte-highlight-switcher": "1.2.13"
  },
  "license": "MIT",
  "bugs": "https://github.com/jill64/npm-demo-layout/issues",
