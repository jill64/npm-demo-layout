  "author": {
    "name": "jill64",
    "email": "contact@jill64.dev",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/npm-demo-layout.git",
    "image": "https://opengraph.githubassets.com/e028d1f3f555882b3485f25a1d3bd3e98f1aa58888c82ea18c28bb173571c648/jill64/npm-demo-layout"
  },
  "publishConfig": {
