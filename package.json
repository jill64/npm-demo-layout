{
  "name": "@jill64/npm-demo-layout",
  "description": "ðŸŽ¨ My Svelte layout template for npm library demo site",
  "version": "1.0.237",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./highlight/languages": {
      "types": "./dist/highlight/languages.d.ts",
      "svelte": "./dist/highlight/languages.js",
      "default": "./dist/highlight/languages.js"
    },
    "./highlight": {
      "types": "./dist/highlight/index.d.ts",
      "svelte": "./dist/highlight/index.js",
      "default": "./dist/highlight/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "prettier": "@jill64/prettier-config",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run check && npx eslint .",
    "format": "npx @jill64/psx",
    "test": "playwright test"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@jill64/eslint-config-svelte": "1.3.3",
    "@jill64/playwright-config": "2.4.0",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/sentry-sveltekit-cloudflare": "1.7.15",
    "@playwright/test": "1.45.3",
    "@sveltejs/adapter-cloudflare": "4.6.1",
    "@sveltejs/kit": "2.5.18",
    "@sveltejs/vite-plugin-svelte": "3.1.1",
    "@jill64/universal-sanitizer": "1.3.0",
    "svelte": "4.2.18",
    "typescript": "5.5.4",
    "vite": "5.3.4"
  },
  "dependencies": {
    "@jill64/svelte-dark-theme": "2.3.88",
    "@jill64/svelte-menu": "1.0.25",
    "@jill64/svelte-ogp": "1.1.31",
    "@jill64/svelte-toast": "1.3.47",
    "@jill64/svelte-sanitize": "1.3.0",
    "svelte-code-copy": "1.0.31",
    "svelte-highlight": "7.7.0",
    "svelte-highlight-switcher": "1.2.14"
  },
  "license": "MIT",
  "bugs": "https://github.com/jill64/npm-demo-layout/issues",
  "homepage": "https://github.com/jill64/npm-demo-layout#readme",
  "author": {
    "name": "jill64",
    "email": "contact@jill64.dev",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/npm-demo-layout.git",
    "image": "https://opengraph.githubassets.com/0cbb294fe52c84a304575e241c2abf510a10f3e6917efd8a82b628d44f1f6823/jill64/npm-demo-layout"
  },
  "publishConfig": {
